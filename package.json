{
  "name": "icuip",
  "version": "0.0.0",
  "description": "Check IP addresses",
  "author": "Ali Waseem <awaseem@pason.com>",
  "private": true,
  "engines": {
    "node": ">= 14.16 <15"
  },
  "main": "./build/src/main.js",
  "scripts": {
    "start": "node ./build/src/main.js",
    "debug": "nodemon --watch ./build --inspect=0.0.0.0:5858 --nolazy ./build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:watch:output": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "prettier": "prettier --write src/",
    "prettier:check": "prettier --check src/",
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "postinstall": "tsc -p tsconfig.release.json"
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "dotenv": "^8.2.0",
    "fastify": "^3.14.2",
    "fastify-helmet": "^5.3.1",
    "fastify-swagger": "^4.5.0",
    "ip-router": "^1.0.0",
    "is-cidr": "^4.0.2",
    "is-ip": "^3.1.0",
    "middie": "^5.2.0",
    "node-fetch": "^2.6.1",
    "tslib": "~2.2.0"
  },
  "devDependencies": {
    "@types/jest": "~26.0.22",
    "@types/node": "~14.14.37",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "~4.21.0",
    "@typescript-eslint/parser": "~4.21.0",
    "eslint": "~7.23.0",
    "eslint-config-prettier": "~8.1.0",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-jest": "~24.3.4",
    "husky": "^6.0.0",
    "jest": "~26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "~2.2.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~26.5.4",
    "tsutils": "~3.21.0",
    "typescript": "^4.2.4"
  }
}
